class Animal{
    String color="white";
    
}
class Dog extends Animal{
    String color="black";
    void printcolor(){
        System.out.println(color);
        System.out.println(super.color);
        
    }
}
class testsuper{
    public static void main(String args[]){
    Dog d1=new Dog();
    d1.printcolor();
    
}
}
//output:
//black
//white
